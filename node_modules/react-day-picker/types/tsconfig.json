{
  "compilerOptions": {
    "module": "commonjs",
    "lib": [
      "es6",
      "dom"
    ],
    "noImplicitAny": false,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "strictFunctionTypes": false
  },
  "include": [
    "./*.d.ts"
  ]
}